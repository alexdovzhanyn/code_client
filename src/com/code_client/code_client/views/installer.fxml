<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.code_client.code_client.controllers.InstallerController" prefHeight="400.0" prefWidth="600.0">
    <HBox GridPane.columnIndex="0" GridPane.rowIndex="0">
        <Button text="CLOSE" fx:id="closeApplicationButton" onAction="#closeApplication">
    </HBox>
    <HBox GridPane.columnIndex="0" GridPane.rowIndex="1">
        <Button text="Go to Ruby Installer" fx:id="rubyPaneButton" onAction="#toggleRubyPane" layoutX="100.0" layoutY="100.0"/>
    </HBox>
</GridPane>
